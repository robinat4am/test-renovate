{
  "extends": [
    "config:recommended"
  ],
  "schedule": ["before 5am every weekday"],
  "repositories": [
    "https://github.com/robinat4am/deployment-new"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["*"],
      "matchRepositories": ["https://github.com/robinat4am/deployment-new"],
      "enabled": true
    }
  ]
}
