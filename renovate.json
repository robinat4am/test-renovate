{
  "extends": [
    "config:recommended"
  ],
  "schedule": ["before 5am every weekday"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    },
    {
      "matchDatasources": ["git-tags"],
      "matchPackageNames": ["deployment-new"],
      "enabled": true
    }
  ]
}
